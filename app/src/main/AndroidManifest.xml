<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.tictactoeapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_tic_tac_toe"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToeApplication">
        <activity android:name=".ResultPage"></activity>
        <activity android:name=".Game_Page" />
        <activity android:name=".TossPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>